{
  "name": "vue3-flip-toolkit",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/david50407/vue3-flip-toolkit.git"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "flip-toolkit": "^7.0.13",
    "vue": "^3.1.5"
  },
  "devDependencies": {
    "@types/node": "^16.4.7",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.1.5",
    "patch-package": "^6.4.7",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vite-plugin-dts": "^0.5.3",
    "vue-loader": "^16.4.0",
    "vue-tsc": "^0.2.2"
  }
}
